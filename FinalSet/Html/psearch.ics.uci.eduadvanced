<h1 align="left">Search for People &amp; Departments <span class="smalltext">[ <a href="/">Basic Search</a> ]</span></h1> 
<form action="" method="get" name="advanced_search" id="advanced_search">
  <label for="searchgroup">Search Group</label>  
  <select name="search_group" size="1" class="adv-field" accesskey="1" tabindex="1" title="Search Group" id="searchgroup">
    <option id="searchgroup_default" value="all" selected>Faculty, Staff &amp; Students</option>
    <option value="facstaff" >Faculty &amp; Staff</option>
    <option value="students" >Students</option>
  </select>
  <br/>
  <label for="form_fname">First Name/Nickname</label>
  <input type="text" name="form_fname" accesskey="2" tabindex="2" id="form_fname" value=""/>
  <br/>
  <label for="form_lname">Last Name</label>
  <input type="text" name="form_lname" accesskey="3" tabindex="3" id="form_lname" value="" />
  <br/>
  <label for="form_email">E-mail</label>
  <input type="text" name="form_email" accesskey="4" tabindex="4" id="form_email" value="" />
  <br/>
  <label for="form_ucinetid">UCInetID</label>
  <input type="text" name="form_ucinetid" accesskey="5" tabindex="5" id="form_ucinetid" value="" />
  <br/>
  <label for="form_phone">Telephone </label>
  <input type="text" name="form_phone" accesskey="6" tabindex="6" id="form_phone" value="" />
  <br/>
  <label for="form_department">Department</label>
  <input type="text" name="form_department" accesskey="7" tabindex="7" id="form_department" value="" />
  <p class="search-buttons">      
    <input type="submit" name="advanced_submit" value="Search" accesskey="8" tabindex="8" id="Submit" />
    <input type="button" name="Reset" value="Clear Form" accesskey="9" tabindex="9" onclick="clearAdvancedForm();"/>
    <input type="hidden" name="form_type" value="advanced_search" />
  </p>
</form>
